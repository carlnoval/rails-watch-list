<div class="movie-list-container container-sm">
  <h3 id="list-header">Available movie lists</h3>

    <div class="list-item-container d-flex flex-wrap">
      <% @lists.each do |list| %>
        <%= link_to list_path(list) do%>
          <div class="list-item">
            <p><%= list.name %></p>
          </div>
        <% end %>
      <% end %>
    </div>
</div>